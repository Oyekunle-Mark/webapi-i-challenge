{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread": true
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0
  }
}
